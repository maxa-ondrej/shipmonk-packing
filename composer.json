{
    "name": "shipmonk/packing",
    "description": "Testing day application stub",
    "version": "1.0.0",
    "scripts": {
        "test": "vendor/bin/phpunit --no-coverage",
        "coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit -c phpunit.xml",
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon",
        "cs-fix": "vendor/bin/php-cs-fixer fix"
    },
    "require": {
        "ext-pdo": "*",
        "ext-json": "*",
        "php": ">=8.4",
        "psr/http-message": "2.*",
        "guzzlehttp/psr7": "2.*",
        "doctrine/orm": "3.*",
        "symfony/cache": "7.*",
        "json-mapper/json-mapper": "^2.25",
        "php-di/php-di": "^7.1",
        "monolog/monolog": "^3.9",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/unit/",
            "Integration\\": "tests/integration/",
            "Tests\\": "tests/helpers/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.75",
        "phpunit/phpunit": "^12.5"
    }
}
